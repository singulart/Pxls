web: mkdir run && cp target/pxls-1.0-SNAPSHOT.jar run && java -jar -Ddatabase.url=$HEROKU_DB_URL -Ddatabase.user=$HEROKU_DB_USER -Ddatabase.pass=$HEROKU_DB_PASS -Dhostname=$HEROKU_HOSTNAME -Dserver.port=$PORT -Dnear.networkId=$NETWORK -Dnear.contract.name=$CONTRACT -Dnear.node.url=$NODE_URL -Dnear.wallet.url=$WALLET_URL -Dnear.helper.url=$HELPER_URL  run/pxls-1.0-SNAPSHOT.jar